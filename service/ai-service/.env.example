# ==============================================
# AI Service 환경설정 예시
# ==============================================

# API Settings
API_TITLE=Prompt Evaluation API
API_VERSION=0.1.0

# ==============================================
# AWS Settings
# ==============================================
# Bedrock 모델용 (US East)
AWS_REGION=us-east-1

# AWS 자격증명 (실제 값으로 교체 필요)
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# ==============================================
# SQS & DynamoDB Settings (서울 리전)
# ==============================================
# SQS 큐 URL
SQS_QUEUE_URL=https://sqs.ap-northeast-2.amazonaws.com/your_account_id/your_queue_name
# DynamoDB 테이블명
DDB_TABLE_NAME=FromProm_Table

# ==============================================
# MCP (Model Context Protocol) API Keys
# ==============================================
# 향상된 환각 탐지를 위한 외부 데이터 소스 API 키들
# 실제 사용시 각 서비스에서 API 키를 발급받아 입력하세요

# Brave Search API (https://api.search.brave.com/)
BRAVE_API_KEY=your_brave_api_key_here

# Tavily Search API (https://tavily.com/)
TAVILY_API_KEY=your_tavily_api_key_here

# Google Custom Search API (https://developers.google.com/custom-search/)
GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here

# 참고: Wikipedia와 Semantic Scholar는 API 키가 필요하지 않습니다

# ==============================================
# Database Settings
# ==============================================
DATABASE_URL=sqlite:///./prompt_eval.db

# ==============================================
# Storage Backend Settings
# ==============================================
# 저장소 백엔드: sqlite, s3, dynamodb_s3
STORAGE_BACKEND=dynamodb_s3
# S3 버킷명 (S3 사용시)
S3_BUCKET=fromprom-s3
# 테이블명 (DynamoDB 사용시, 서울 리전)
TABLE_NAME=FromProm_Table
DDB_TABLE_NAME=FromProm_Table

# ==============================================
# Cache Settings
# ==============================================
CACHE_ENABLED=true
CACHE_TTL=3600

# ==============================================
# AWS Bedrock Agent IDs (Strands Framework)
# ==============================================
# Supervisor Agent ID
SUPERVISOR_AGENT_ID=

# Specialist Agent IDs
TOKEN_USAGE_AGENT_ID=
INFO_DENSITY_AGENT_ID=
CONSISTENCY_AGENT_ID=
MODEL_VARIANCE_AGENT_ID=
RELEVANCE_AGENT_ID=
HALLUCINATION_AGENT_ID=

# Agent Alias ID (모든 Agent 공통)
AGENT_ALIAS_ID=TSTALIASID

# ==============================================
# 테스트/개발 모드 설정
# ==============================================
# Mock 모드: true면 AWS 없이 테스트 가능
MOCK_MODE=false

# Agent 파이프라인: true면 Tool 기반 Agent 파이프라인 사용
USE_AGENT_PIPELINE=true

# ==============================================
# 알고리즘 파라미터
# ==============================================
# 일관성 계산에서 최대 거리 패널티 가중치
ALPHA=0.2

# ==============================================
# 실제 AWS 사용시 설정 방법
# ==============================================
# 1. MOCK_MODE=false로 변경
# 2. AWS_ACCESS_KEY_ID, AWS_SECRET_ACCESS_KEY 입력
# 3. SQS_QUEUE_URL 입력 (SQS Worker 사용시)
# 4. STORAGE_BACKEND를 s3 또는 dynamodb_s3로 변경 (필요시)
# 5. 필요한 API 키들 입력 (BRAVE, TAVILY, GOOGLE 등)
