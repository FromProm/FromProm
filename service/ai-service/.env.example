# API Settings
API_TITLE=Prompt Evaluation API
API_VERSION=0.1.0

# AWS Settings (실제 사용시 입력 필요)
AWS_REGION=us-east-1
AWS_REGION_SQS_DDB=ap-northeast-2
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# SQS & DynamoDB Settings (서울 리전)
SQS_QUEUE_URL=https://sqs.ap-northeast-2.amazonaws.com/your_account_id/your_queue_name
DDB_TABLE_NAME=FromProm_Table

# S3 Settings
S3_BUCKET=fromprom-s3

# Lambda Settings
AGENT_ARN=arn:aws:bedrock-agentcore:us-east-1:your_account_id:runtime/your_agent_name

# Search API Keys
BRAVE_API_KEY=your_brave_api_key_here
TAVILY_API_KEY=your_tavily_api_key_here
GOOGLE_SEARCH_API_KEY=your_google_search_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_google_search_engine_id_here

# Database
DATABASE_URL=sqlite:///./prompt_eval.db

# Storage Backend
STORAGE_BACKEND=sqlite
S3_BUCKET_NAME=your_s3_bucket_name
TABLE_NAME=prompt-evaluations

# Cache Settings
CACHE_ENABLED=true
CACHE_TTL=3600

# Mock Mode (개발/테스트용)
MOCK_MODE=false

# Consistency Parameters
ALPHA=0.2